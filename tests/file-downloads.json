{
  "scenarios":[
    {
      "name": "Download file contents",
      "method": "get-url",
      "enabled": true,
      "request-parameters": {
        "!url": "[download-location]"
      },
      "test-setup": [
        { "canned-request": "create-file-item" },
        {
          "method": "download-item-content",
          "request-parameters": { "{item-id}": "[item-id]" },
          "capture": {
            "[download-location]": "Location:"
          },
          "allowed-status-codes": [302]
        }
      ]
    },
    {
      "name": "Resume a download with Range",
      "method": "download-item-partial",
      "enabled": true,
      "tags": [ "service.graph" ],

      "request-parameters":
      {
        "!url": "[download-url]",
        "Range:": "bytes=4-"
      },
      "test-setup": [
        {
          "method": "delete-item",
          "request-parameters":
          {
            "{item-id}": "root:/test-folder/test-file-range.txt"
          },
          "allowed-status-codes": [204, 404]
        },
        {
          "method": "upload-via-put",
          "request-parameters": {
            "!url": "/me/drive/root:{item-path}:/content",
            "{item-path}": "/test-folder/test-file-range.txt"
          },
          "capture": {
            "[stored-item-id]": "$.id"
          }
        },
        {
          "method": "download-item-content",
          "request-parameters": {"{item-id}": "[stored-item-id]" },
          "allowed-status-codes": [302],
          "capture": { "[download-url]": "Location:" }
        }
      ]
    },
    {
      "name": "Convert to PDF",
      "method": "convert-item-content",
      "enabled": true,
      "tags": [ "service.graph" ],

      "request-parameters": {
        "{item-id}": "[item-id]",
        "{format}": "pdf"
      },
      "test-setup": [
        { "canned-request": "create-word-document" } 
      ]
    },
    {
      "name": "Download as PDF",
      "method": "get-url",
      "enabled": true,
      "request-parameters": {
        "!url": "[redirect-location]"
      },
      "allowed-status-codes": [ 200 ],
      "expectations": {
        "Content-Type:": "application/pdf"
      },
      "test-setup": [
        { "canned-request": "create-word-document" },
        {
          "method": "convert-item-content",
          "request-parameters": {
            "{item-id}": "[item-id]",
            "{format}": "pdf"
          },
          "capture": {
            "[redirect-location]": "Location:"
          },
          "allowed-status-codes": [302]
        } 
      ]
    }
  ],
